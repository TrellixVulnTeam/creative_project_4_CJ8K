n        stars: this.grade\n      }\n    },\n    methods: {\n      rate(star) {\n        if (\n          typeof star === 'number' &&\n          star <= this.maxStars &&\n          star >= 0\n        )\n          this.stars = this.stars === star ? star - 1 : star\n      }\n    }\n  }\n</script>\n\n<style scoped lang=\"scss\">\n  $active-color: #f3d23e;\n\n  .rating {\n    font-family: 'Avenir', Helvetica, Arial, sans-serif;\n    font-size: 22px;\n    color: #a7a8a8;\n  }\n  .list {\n    margin: 0 0 5px 0;\n    padding: 0;\n    list-style-type: none;\n    &:hover {\n      .star {\n        color: $active-color;\n      }\n    }\n  }\n  .star {\n    display: inline-block;\n    cursor: pointer;\n    &:hover {\n      &~.star {\n        &:not(.active) {\n          color: inherit;\n        }\n      }\n    }\n  }\n  .active {\n    color: $active-color;\n  }\n</style>\n"]}]}